    "import matplotlib.pyplot as plt\n",
    "import matplotlib.patches as patches\n",
    "\n",
    "fig = plt.figure()\n",
    "ax = fig.add_subplot(111)\n",
    "\n",
    "triangle = patches.Polygon([[0,0],[1,0],[0.5,1]], closed=True, fill=False)\n",
    "ax.add_patch(triangle)\n",
    "\n",
    "plt.xlim(-1,2)\n",
    "plt.ylim(-1,2)\n",
    "plt.grid(True)\n",
    "plt.show()\n"
   ]
